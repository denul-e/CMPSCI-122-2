#include <stdio.h>

int main() {
    int num1, num2, divide, remainder;

    // Prompt the user to enter two integers
    printf("Enter two integers: ");
    scanf("%d %d", &num1, &num2);

    // Perform integer division and find the remainder
    divide = num1 / num2;
    remainder = num1 % num2;

    // Output the results
    printf("Divide = %d  Remainder = %d\n", divide, remainder);

    return 0;
}
